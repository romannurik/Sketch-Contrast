{
  "name": "sketch-contrast",
  "description": "A Sketch plugin to show a pass/fail type contrast report.",
  "version": "0.1.5",
  "engines": {
    "sketch": ">=49.0.0"
  },
  "skpm": {
    "name": "Contrast",
    "manifest": "src/manifest.json",
    "main": "Contrast.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romannurik/Sketch-Contrast.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/romannurik/Sketch-Contrast/issues"
  },
  "homepage": "https://github.com/romannurik/Sketch-Contrast#readme",
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@skpm/builder": "^0.5.9"
  },
  "author": "Roman Nurik <roman@nurik.net>",
  "dependencies": {
    "@skpm/fs": "^0.1.2",
    "@skpm/path": "^0.1.0",
    "mocha-js-delegate": "^0.1.1"
  }
}
